set PREVIEW_MODE V:/DPT-SoJ.png
set SOJ_VERSION 0.2.1
set APPPATH 0:/3ds/DragonPunkTeam
set OUTPATH 0:/3ds/DragonPunkTeam/SoJ
set DIGITAL A:/title/00040000/0018FA00/content/00000002.app

echo "Benvenuto nel patcher di Spirit of Justice.\nVerra' installata la versione $[SOJ_VERSION] della\ntraduzione a cura del DragonPunk Team.\n\nL'installazione durera' qualche minuto."

if not find $[APPPATH]/SoJ-Patch-v$[SOJ_VERSION].bps NULL
    echo "Impossibile trovare\nSD:/3DS/DragonPunkTeam/SoJ-Patch-v$[SOJ_VERSION].bps\nAssicurati di aver estratto tutti i file\ndallo .zip della patch e riprova."
    poweroff
elif not sha $[APPPATH]/SoJ-Patch-v$[SOJ_VERSION].bps "FA41F574646AE70C1B652C66F7994AADF220E578F73DEB6931D05913C70D213C"
	echo "La patch Ã¨ danneggiata.\nAssicurati di aver estratto tutti i file\ndallo .zip della patch e riprova."
	poweroff
end

mkdir 0:/3ds
mkdir $[APPPATH]
mkdir $[OUTPATH]

if find $[DIGITAL] NULL
    extrcode $[DIGITAL] $[OUTPATH]/SOJ-ENG.bin
    imgmount $[DIGITAL]
    set SOURCE G:
else
    echo "Impossibile trovare una copia di Spirit of\nJustice sul 3DS. Assicurati che il gioco sia\ninstallato correttamente e riavvia il patcher."
    reboot
end

if find 0:/luma/titles/000400000018FA00 NULL
